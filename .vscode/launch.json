  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Launch encoder_tests_split_no_left",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/qmk_firmware/.build/test/encoder_split_no_left.elf",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/qmk_firmware/.build/test",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "(gdb) Launch encoder_tests_split_no_right",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/qmk_firmware/.build/test/encoder_split_no_right.elf",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/qmk_firmware/.build/test",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "Python: painter-convert-graphics",
      "type": "python",
      "cwd": "${workspaceRoot}/qmk_firmware",
      "request": "launch",
      "program": "E:\\QMK_MSYS\\mingw64\\bin\\qmk.exe",
      "args": [
        "painter-convert-graphics",
        "--input",
        "tzarc-cyclone/graphics/lock-caps.png",
        "--format",
        "mono16",
        "--rle"
      ],
      "console": "integratedTerminal",
      "env": {
        "MSYSTEM": "MINGW64",
        "CHERE_INVOKING": "1"
      }
    },
    {
      "name": "Python: painter-make-font-image",
      "type": "python",
      "cwd": "${workspaceRoot}",
      "request": "launch",
      "program": "E:\\QMK_MSYS\\mingw64\\bin\\qmk.exe",
      "args": [
        "painter-make-font-image",
        "--font",
        "tzarc-cyclone/graphics/NotoSans-ExtraCondensedBold.ttf",
        "--size",
        "16",
        "--output",
        "tzarc-cyclone/graphics/noto.png",
        "--unicode-glyphs",
        "ĄȽɂɻɣɈʣ"
      ],
      "console": "integratedTerminal",
      "env": {
        "MSYSTEM": "MINGW64",
        "CHERE_INVOKING": "1"
      }
    },
    {
      "name": "Python: painter-convert-font-image",
      "type": "python",
      "cwd": "${workspaceRoot}",
      "request": "launch",
      "program": "E:\\QMK_MSYS\\mingw64\\bin\\qmk.exe",
      "args": [
        "painter-convert-font-image",
        "--input",
        "tzarc-cyclone/graphics/noto.png",
        "--format",
        "mono4",
        "--unicode-glyphs",
        "ĄȽɂɻɣɈʣ"
      ],
      "console": "integratedTerminal",
      "env": {
        "MSYSTEM": "MINGW64",
        "CHERE_INVOKING": "1"
      }
    },
    {
      "name": "Black Magic Probe (L412, cortex-debug)",
      "type": "cortex-debug",
      "request": "attach",
      "cwd": "${workspaceRoot}/qmk_firmware",
      "svdFile": "${workspaceRoot}/svd/STM32L4x2.svd",
      "servertype": "bmp",
      "interface": "swd",
      "device": "STM32L4x2",
      "linux": {
        "BMPGDBSerialPort": "/dev/ttyACM0",
        "executable": "${workspaceRoot}/qmk_firmware/.build/qp_test_stm32l412kb_default.elf",
        "armToolchainPath": "/home/nickb/gcc-arm/gcc-arm-none-eabi-10-2020-q4-major/bin"
      },
      "windows": {
        "BMPGDBSerialPort": "COM6",
        "executable": "${workspaceRoot}\\qmk_firmware\\.build\\qp_test_stm32l412kb_default.elf",
        "armToolchainPath": "E:\\gcc-arm-none-eabi-10.3-2021.10\\bin"
      },
      "showDevDebugOutput": "parsed",
      "preAttachCommands": ["set mem inaccessible-by-default off"],
      "postAttachCommands": []
    },
    {
      "name": "Black Magic Probe (Ghoul, cortex-debug)",
      "type": "cortex-debug",
      "request": "attach",
      "cwd": "${workspaceRoot}/qmk_firmware",
      "svdFile": "${workspaceRoot}/svd/STM32F405.svd",
      "servertype": "bmp",
      "interface": "swd",
      "device": "STM32F405",
      "linux": {
        "BMPGDBSerialPort": "/dev/ttyACM0",
        "executable": "${workspaceRoot}/qmk_firmware/.build/handwired_tzarc_ghoul_stm32_default.elf",
        "armToolchainPath": "/home/nickb/gcc-arm/gcc-arm-none-eabi-10-2020-q4-major/bin"
      },
      "windows": {
        "BMPGDBSerialPort": "COM6",
        "executable": "${workspaceRoot}\\qmk_firmware\\.build\\handwired_tzarc_ghoul_stm32_default.elf",
        "armToolchainPath": "E:\\gcc-arm-none-eabi-10.3-2021.10\\bin"
      },
      "showDevDebugOutput": "parsed",
      "preAttachCommands": ["set mem inaccessible-by-default off"],
      "postAttachCommands": []
    }
  ]
}
