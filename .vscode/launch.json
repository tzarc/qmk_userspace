{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Launch wear_leveling_8byte",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/qmk_firmware/.build/test/wear_leveling_8byte.elf",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/qmk_firmware/.build/test",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      //"preLaunchTask": "build wear_leveling tests",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "Python: json2c, Ghoul/RP2040",
      "type": "python",
      "cwd": "${workspaceRoot}",
      "request": "launch",
      "args": [
        "json2c",
        "ghoul-stm32-encodermap.json",
        "-o",
        "-"
      ],
      "console": "integratedTerminal",
      "linux": {
        "env": {
          "PYTHONPATH": "/home/nickb/.local/venv_qmk/bin/python3"
        },
        "program": "/home/nickb/.local/venv_qmk/bin/qmk"
      },
      "windows": {
        "program": "E:\\QMK_MSYS\\mingw64\\bin\\qmk.exe",
        "env": {
          "MSYSTEM": "MINGW64",
          "CHERE_INVOKING": "1"
        }
      }
    },
    {
      "name": "Python: generate-keyboard-c, Ghoul/RP2040",
      "type": "python",
      "cwd": "${workspaceRoot}/qmk_firmware",
      "request": "launch",
      "args": [
        "generate-keyboard-c",
        "-kb",
        "handwired/tzarc/ghoul/rev1/rp2040",
        "-o",
        "-"
      ],
      "console": "integratedTerminal",
      "linux": {
        "env": {
          "PYTHONPATH": "/home/nickb/.local/venv_qmk/bin/python3"
        },
        "program": "/home/nickb/.local/venv_qmk/bin/qmk"
      },
      "windows": {
        "program": "E:\\QMK_MSYS\\mingw64\\bin\\qmk.exe",
        "env": {
          "MSYSTEM": "MINGW64",
          "CHERE_INVOKING": "1"
        }
      }
    },
    {
      "name": "Black Magic Probe (Djinn G474 default keymap, cortex-debug)",
      "type": "cortex-debug",
      "request": "attach",
      "cwd": "${workspaceRoot}/qmk_firmware",
      "svdFile": "${workspaceRoot}/svd/STM32G474xx.svd",
      "servertype": "bmp",
      "interface": "swd",
      "device": "STM32G474",
      "linux": {
        "BMPGDBSerialPort": "/dev/ttyACM0",
        "executable": "${workspaceRoot}/qmk_firmware/.build/tzarc_djinn_rev1_default.elf",
        "armToolchainPath": "/home/nickb/gcc-arm/gcc-arm-none-eabi-10-2020-q4-major/bin"
      },
      "windows": {
        "BMPGDBSerialPort": "COM5",
        "executable": "${workspaceRoot}\\qmk_firmware\\.build\\tzarc_djinn_rev1_default.elf",
        "armToolchainPath": "E:\\gcc-arm-none-eabi-10.3-2021.10\\bin"
      },
      "showDevDebugOutput": "parsed",
      "preAttachCommands": ["set mem inaccessible-by-default off"],
      "postAttachCommands": []
    },
    {
      "name": "Black Magic Probe (L412, cortex-debug)",
      "type": "cortex-debug",
      "request": "attach",
      "cwd": "${workspaceRoot}/qmk_firmware",
      "svdFile": "${workspaceRoot}/svd/STM32L4x2.svd",
      "servertype": "bmp",
      "interface": "swd",
      "device": "STM32L4x2",
      "linux": {
        "BMPGDBSerialPort": "/dev/ttyACM0",
        "executable": "${workspaceRoot}/qmk_firmware/.build/qp_test_stm32l412kb_default.elf",
        "armToolchainPath": "/home/nickb/gcc-arm/gcc-arm-none-eabi-10-2020-q4-major/bin"
      },
      "windows": {
        "BMPGDBSerialPort": "COM6",
        "executable": "${workspaceRoot}\\qmk_firmware\\.build\\qp_test_stm32l412kb_default.elf",
        "armToolchainPath": "E:\\gcc-arm-none-eabi-10.3-2021.10\\bin"
      },
      "showDevDebugOutput": "parsed",
      "preAttachCommands": ["set mem inaccessible-by-default off"],
      "postAttachCommands": []
    },
    {
      "name": "Black Magic Probe (Ghoul STM32F4, cortex-debug)",
      "type": "cortex-debug",
      "request": "attach",
      "cwd": "${workspaceRoot}/qmk_firmware",
      "svdFile": "${workspaceRoot}/svd/STM32F405.svd",
      "servertype": "bmp",
      "interface": "swd",
      "device": "STM32F405",
      "linux": {
        "BMPGDBSerialPort": "/dev/ttyACM0",
        "executable": "${workspaceRoot}/qmk_firmware/.build/tzarc_ghoul_rev1_stm32_tzarc.elf",
        "armToolchainPath": "/home/nickb/gcc-arm/gcc-arm-none-eabi-10-2020-q4-major/bin"
      },
      "windows": {
        "BMPGDBSerialPort": "COM4",
        "executable": "${workspaceRoot}\\qmk_firmware\\.build\\tzarc_ghoul_rev1_stm32_tzarc.elf",
        "armToolchainPath": "E:\\gcc-arm-none-eabi-10.3-2021.10\\bin"
      },
      "showDevDebugOutput": "parsed",
      "preAttachCommands": ["set mem inaccessible-by-default off"],
      "postAttachCommands": []
    },
    {
      "name": "Black Magic Probe (Ghoul RP2040, cortex-debug)",
      "type": "cortex-debug",
      "request": "attach",
      "cwd": "${workspaceRoot}/qmk_firmware",
      "svdFile": "${workspaceRoot}/qmk_firmware/lib/pico-sdk/rp2040/hardware_regs/rp2040.svd",
      "servertype": "bmp",
      "interface": "swd",
      "device": "RP2040",
      "linux": {
        "BMPGDBSerialPort": "/dev/ttyACM0",
        "executable": "${workspaceRoot}/qmk_firmware/.build/handwired_tzarc_ghoul_rev1_rp2040_default.elf",
        "armToolchainPath": "/home/nickb/gcc-arm/gcc-arm-none-eabi-10-2020-q4-major/bin"
      },
      "windows": {
        "BMPGDBSerialPort": "COM6",
        "executable": "${workspaceRoot}\\qmk_firmware\\.build\\handwired_tzarc_ghoul_rev1_rp2040_default.elf",
        "armToolchainPath": "E:\\gcc-arm-none-eabi-10.3-2021.10\\bin"
      },
      "showDevDebugOutput": "parsed",
      "preAttachCommands": ["set mem inaccessible-by-default off"],
      "postAttachCommands": []
    }
  ]
}
