{
  "files.exclude": {
    "**/.build": true,
    "**/*.hex": true,
    "**/*.bin": true
  },
  "git.ignoreLimitWarning": true,
  "[markdown]": {
    "editor.trimAutoWhitespace": false,
    "files.trimTrailingWhitespace": false
  },
  "files.associations": {
    "array": "cpp",
    "system_error": "c",
    "regex": "c"
  }
}